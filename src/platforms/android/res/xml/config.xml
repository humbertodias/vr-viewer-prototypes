<?xml version='1.0' encoding='utf-8'?>
<widget id="br.vr.viewer.models" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="org.crosswalk.engine.XWalkWebViewEngine" />
    <preference default="18+" name="xwalkVersion" />
    <preference default="xwalk_core_library_canary:17+" name="xwalkLiteVersion" />
    <preference default="--disable-pull-to-refresh-effect" name="xwalkCommandLine" />
    <preference default="embedded" name="xwalkMode" />
    <preference default="true" name="xwalkMultipleApk" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <icon src="www/favicon.png" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <icon height="48" src="resources/icon-48x48.png" width="48" />
    <icon height="72" src="resources/icon-72x72.png" width="72" />
    <icon height="96" src="resources/icon-96x96.png" width="96" />
    <icon height="144" src="resources/icon-144x144.png" width="144" />
    <icon height="192" src="resources/icon-192x192.png" width="192" />
    <splash density="port-mdpi" height="470" src="resources/splash-320x470.png" width="320" />
    <splash density="land-mdpi" height="320" src="resources/splash-470x320.png" width="470" />
    <splash density="port-hdpi" height="640" src="resources/splash-480x640.png" width="480" />
    <splash density="land-hdpi" height="480" src="resources/splash-640x480.png" width="640" />
    <splash density="port-xhdpi" height="960" src="resources/splash-720x960.png" width="720" />
    <splash density="land-xhdpi" height="720" src="resources/splash-960x720.png" width="960" />
    <splash height="1440" src="resources/splash-1080x1440.png" width="1080" />
    <splash height="1080" src="resources/splash-1440x1080.png" width="1440" />
    <name>VR Viewer 3D Models</name>
    <description>
        Carregue dinamicamente seus modelos 3D e visualize-os com efeito estereosc√≥pico.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="fullscreen" value="true" />
    <preference name="orientation" value="landscape" />
    <preference name="xwalkVersion" value="18+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
